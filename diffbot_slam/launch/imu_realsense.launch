<launch>
  <node name="imu_filter" pkg="imu_filter_madgwick" type="imu_filter_node">
    <param name="use_mag" type="bool" value="false"/>
    <param name="publish_tf" type="bool" value="false"/>
    <param name="world_frame" type="string" value="enu"/>
    <remap from="/imu/data_raw" to="/camera/imu"/>
  </node>

  <node name="imu_optical_to_imu" pkg="tf" type="static_transform_publisher" 
        args="0 0 0 0 -1.5707963 1.5707963 camera_imu_optical_frame imu_link 200" />

  <!--
  <node pkg="robot_localization" type="ekf_localization_node" name="ekf_se" clear_params="true">
    <rosparam command="load" file="$(find diffbot_slam)/config/localization_vins.yaml" />

    <remap from="odometry/filtered" to="/diffbot/odom_combined"/>


  </node>
  -->
</launch>
